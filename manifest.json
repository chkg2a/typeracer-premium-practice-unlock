{
  "manifest_version": 3,
  "name": "Typeracer Premium Practice Unlock",
  "description": "Grants you to save Premium Races and Run EasyTexts on Practice mode",
  "version": "1.0",
  "icons": {
    "16": "images/th-16.png",
    "32": "images/th-32.png",
    "48": "images/th-48.png",
    "128": "images/th-128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "storage"]
}
